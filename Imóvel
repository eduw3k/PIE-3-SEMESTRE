package javaapplication1.model;

public class Imovel {
    private int id;
    private String descricao;

    public Imovel() {}
    public Imovel(int id, String descricao){ this.id = id; this.descricao = descricao; }
    public int getId(){ return id; }
    public String getDescricao(){ return descricao; }
    @Override public String toString(){ return "Im√≥vel #" + id + " - " + descricao; }
}
